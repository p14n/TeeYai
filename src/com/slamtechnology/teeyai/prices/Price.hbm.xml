<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
    <class name="com.slamtechnology.teeyai.prices.Price" table="PRICE_HISTORY">
        <id name="priceID">
        	<generator class="increment"/>
        </id>
        <property name="price"/>
        <property name="name"/>
        <property name="timestamp"/>
    
	
    </class>
</hibernate-mapping>